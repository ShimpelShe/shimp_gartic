{
  "manifest_version": 3,
  "name": "Shimple's Gartic Extension",
  "version": "1.0",
  "description": "Weew!",
  "permissions": ["activeTab", "scripting", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://garticphone.com/*"],
      "js": ["content.js"]
    }
  ]
}
